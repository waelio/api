{
  "name": "waelio-api",
  "version": "0.1.0",
  "type": "module",
  "description": "Waelio API - Nitro TypeScript with OpenAPI",
  "packageManager": "pnpm@9.15.0",
  "pnpm": {
    "ignoredBuiltDependencies": [
      "esbuild"
    ],
    "onlyBuiltDependencies": [
      "@parcel/watcher"
    ]
  },
  "scripts": {
    "dev": "pnpm dev:api & pnpm dev:ui",
    "dev:api": "nitro dev --port 4000",
    "dev:ui": "cd frontend && pnpm dev",
    "build": "nitro build",
    "start": "node .output/server/index.mjs",
    "preview": "node .output/server/index.mjs",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui"
  },
  "dependencies": {
    "netlify-cli": "^23.13.0",
    "nitropack": "^2.12.9"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  }
}